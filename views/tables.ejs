<div class="content">
  <div class="row">
    <div class="col-md-12">
      <div class="card">
        <div class="card-header">
          <h4 class="card-title">Trabajadores</h4>
          <p class="category">Trabajadores Registrados</p>
        </div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table tablesorter" id="">
              <thead class="text-primary">
                <tr>
                  <th>Nombres</th>
                  <th>Apellidos</th>
                  <th>Celular</th>
                  <th class="text-center">Opciones</th>
                </tr>
              </thead>
              <% trabajadores.forEach((trabajadores)=>{ %> %>
              <tbody>
                <tr>
                  <td><%= trabajadores.nombres %></td>
                  <td><%= trabajadores.apellidos %></td>
                  <td><%= trabajadores.celular %></td>
                  <td class="text-center">
                    <a href="#" class="btn btn-success btn-icon btn-sm">
                      <i class="fa fa-edit"></i>
                    </a>
                    <br />
                    <br />
                    <a href="#" class="btn btn-danger btn-icon btn-sm">
                      <i class="fa fa-trash"></i>
                    </a>
                  </td>
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- <div class="col-md-12">
        <div class="card  card-plain">


        </div>
      </div> -->
  </div>
</div>
